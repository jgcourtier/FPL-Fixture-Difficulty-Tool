   "source": [
    "# Calculate win probability with elo formula.\n",
    "\n",
    "def win_prob(ELOa,ELOb):\n",
    "    return (1 / (1+10**(((ELOa-ELOb)/400))))\n",
    "\n",
    "\n",
    "df['HomeELO'] = df['HomeELO'].fillna(0)\n",
    "df['AwayELO'] = df['AwayELO'].fillna(0)\n",
    "\n",
    "df['awayP'] = df[['HomeELO','AwayELO']].apply(lambda x: win_prob(*x)*0.7,axis=1) # Includes negative multiplier for difference in Home/Away win rates.\n",
    "df['homeP'] = df[['AwayELO','HomeELO']].apply(lambda x: win_prob(*x),axis=1)\n",
    "\n",
    "pd.set_option('display.max_columns',None)\n",
    "df.head()\n"
   ]
  },
  {
   "cell_type": "code",
