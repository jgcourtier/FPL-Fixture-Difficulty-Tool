   "source": [
    "# Calculate win probability with elo formula.\n",
    "\n",
    "def win_prob(ELOa,ELOb):\n",
    "    return (1 / (1+10**(((ELOa-ELOb)/400))))\n",
    "\n",
    "\n",
    "df['HomeELO'] = df['HomeELO'].fillna(0)\n",
    "df['AwayELO'] = df['AwayELO'].fillna(0)\n",
    "\n",
    "df['awayP'] = df[['HomeELO','AwayELO']].apply(lambda x: win_prob(*x)*0.7,axis=1) # Includes negative multiplier for difference in Home/Away win rates.\n",
    "df['homeP'] = df[['AwayELO','HomeELO']].apply(lambda x: win_prob(*x),axis=1)\n",
    "\n",
    "pd.set_option('display.max_columns',None)\n",
    "df.head()\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "ebd55964",
   "metadata": {},
   "outputs": [],
   "source": [
    "df['kickoff_time'] = pd.to_datetime(df['kickoff_time'],format='%Y-%m-%dT%H:%M:%SZ')\n",
    "next_gw = min(df['event'],default=1)\n",
    "\n",
    "# show_gws = 5\n",
    "show_gws = int(input('How many gameweeks ahead would you like to see?'))\n",
    "\n",
    "teamlist = ranking['Club'].tolist()\n",
    "upcoming = []\n",
    "\n",
    "for team in teamlist:\n",
    "    teamdf = df[['event','kickoff_time','HomeTeam','AwayTeam','HomeELO','AwayELO','awayP','homeP']][(df['HomeTeam'] == f'{team}') | (df['AwayTeam'] == f'{team}')]\n",
    "    teamdf['Team'] = team\n",
    "    teamdf['isHome'] = np.where(teamdf['HomeTeam'] == f'{team}',True,False)\n",
    "    teamdf['Opponent'] = np.where(teamdf['isHome'] == True,teamdf['AwayTeam'].apply(lambda x: f'{x} (H)'),teamdf['HomeTeam'].apply(lambda x: f'{x} (A)'))\n",
    "    teamdf['OpponentELO'] = np.where(teamdf['HomeTeam'] == f'{team}',teamdf['AwayELO'],teamdf['HomeELO'])\n",
    "    teamdf['ELO'] = np.where(teamdf['HomeTeam'] == f'{team}',teamdf['HomeELO'],teamdf['AwayELO'])\n",
    "    teamdf['winProb'] = np.where(teamdf['HomeTeam'] == f'{team}',teamdf['homeP'],teamdf['awayP'])\n",
    "    teamdf = teamdf.loc[(teamdf['event'] >= next_gw) & (teamdf['event'] < (next_gw+show_gws))]\n",
    "    teamdf = teamdf.drop(columns=['kickoff_time','HomeTeam','AwayTeam','HomeELO','AwayELO','awayP','homeP','isHome'])\n",
    "\n",
    "    upcoming.append(teamdf)\n",
    "\n",
    "upcomingdf = pd.concat(upcoming,ignore_index=True)\n"
   ]
  },
