   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import requests\n",
    "from datetime import datetime\n",
    "\n",
    "elo_url = 'http://api.clubelo.com/'\n",
    "FPL_url = 'https://fantasy.premierleague.com/api/'\n",
    "\n",
    "date = datetime.today().strftime('%Y-%m-%d')\n",
    "\n",
    "elo_r = requests.get(elo_url+date)\n",
    "fixtures_r = requests.get(FPL_url+'fixtures?future=1').json()\n",
    "teams_r = requests.get(FPL_url+'bootstrap-static/').json()\n",
    "\n",
    "\n",
    "with open('Elo_output.txt','wb') as file:\n",
    "    file.write(elo_r.content)\n",
    "\n",
    "\n",
    "ranking = pd.read_csv('Elo_output.txt')\n",
    "ranking = ranking.loc[(ranking['Country'] == 'ENG') & (ranking['Level'] == 1)]\n",
    "\n",
    "fixtures = pd.json_normalize(fixtures_r)\n",
    "teams = pd.json_normalize(teams_r['teams'])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 38,
   "id": "d7be380c",
   "metadata": {},
   "outputs": [],
   "source": [
    "team_dict = {\"Forest\": \"Nott'm Forest\", \"Tottenham\": \"Spurs\", \"Man United\": \"Man Utd\"} # Normalize names across ClubElo and FPL, mapping to FPL team names.\n",
    "\n",
    "ranking = ranking.replace({'Club': team_dict})"
   ]
  },
